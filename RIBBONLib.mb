	marrControls(nCtrl) = MICtrlCollInsertIntStrStrInt(theMapMTBControlsColl, nPosition, sControlName, sCaption, nControlType)

	If sKeytip <> "" Then
		'Set the KeyTip for the Control. KeyTip: The keyboard shortcut for the Control.
