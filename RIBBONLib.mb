	rbnGroup		= NULL_PTR
	rbnGroupsColl	= NULL_PTR
	rbnTabFound	= NULL_PTR
	rbnGroupFound	= NULL_PTR
	rbnTab		= NULL_PTR

	Exit Function
'-------------------------
ErrorOccured:
	Call ERRCreate(Err(), Error$(), "RBNTabGroupRemove: " & sGroupName & " from tab " & sTabName)
