	rbnTabFound			= NULL_PTR
	rbnGroupFound			= NULL_PTR
	rbnControlFound		= NULL_PTR
	rbnControlGroupFound	= NULL_PTR

	Exit Function
'-------------------------
ErrorOccured:
	Call ERRCreate(Err(), Error$(), "RBNDropDButtonGroupRemove: " & sDropDButtonGroupName & " on tab " & sTabName)
	Call ERRShow()

End Function

